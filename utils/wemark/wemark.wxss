.nprogress-busy .markdown-body {
	opacity: .5;
}
.markdown-body {
	font-family:-apple-system, BlinkMacSystemFont, "Segoe UI", Helvetica, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol", "PingFang SC", "Microsoft YaHei UI";
	font-size:28rpx;
	line-height:1.7;
	word-wrap:break-word;
	transition: .3s;
	-webkit-transition: .3s;
	padding: 40rpx;
	text-align: left;
}
.markdown-body::before {
	display:table;
	content:""
}
.markdown-body::after {
	display:table;
	clear:both;
	content:""
}
.markdown-body .a {
	color:#3688ff !important;
	padding: 5rpx;
	text-decoration:none;
	border-radius: 6rpx;
	-webkit-transition: .2s;
}
.markdown-body .a:active {
	background: rgba(0, 0, 0, .05);
}
.markdown-body .absent {
	color:#c00
}
.markdown-body .anchor {
	display: none;
}
.markdown-body .anchor:focus {
	outline:none
}
.markdown-body .p,.markdown-body .blockquote,.markdown-body .ul,.markdown-body .ol,.markdown-body .dl,.markdown-body .table,.markdown-body .pre {
	margin-top:0;
	margin-bottom:32rpx
}
.markdown-body .hr {
    width: 100rpx;
    height: 1rpx;
    margin: 30rpx 0;
    border: 0;
    background: rgba(0, 0, 0, .05);
}
.markdown-body .blockquote_p {
	padding: 0 1em;
    color: #666;
    border-left: 0.25em solid #f5f5f5;
    margin-left: 0;
    margin-right: 0;
    margin-bottom: 1em;
}
.markdown-body .blockquote_p + .blockquote_p {
    margin-top: -1em;
    padding-top: 1em;
}
.markdown-body .kbd {
	display:inline-block;
	padding:6rpx 10rpx;
	font-size:22rpx;
	line-height:20rpx;
	color:#666;
	vertical-align:middle;
	background-color:#fcfcfc;
	border:solid 1rpx #ccc;
	border-bottom-color:#bbb;
	border-radius:6rpx;
	box-shadow:inset 0 -1rpx 0 #bbb
}
.markdown-body .loweralpha {
	list-style-type:lower-alpha
}
.markdown-body .h1,.markdown-body .h2,.markdown-body .h3,.markdown-body .h4,.markdown-body .h5,.markdown-body .h6 {
	margin-top:48rpx;
	margin-bottom:32rpx;
	font-weight:normal;
	line-height:1.25;
	-user-select: none;
	-webkit-user-select: none;
}
.markdown-body .h1 .octicon-link,.markdown-body .h2 .octicon-link,.markdown-body .h3 .octicon-link,.markdown-body .h4 .octicon-link,.markdown-body .h5 .octicon-link,.markdown-body .h6 .octicon-link {
	color:#000;
	vertical-align:middle;
	visibility:hidden
}
.markdown-body .h1:hover .anchor,.markdown-body .h2:hover .anchor,.markdown-body .h3:hover .anchor,.markdown-body .h4:hover .anchor,.markdown-body .h5:hover .anchor,.markdown-body .h6:hover .anchor {
	text-decoration:none
}
.markdown-body .h1:hover .anchor .octicon-link,.markdown-body .h2:hover .anchor .octicon-link,.markdown-body .h3:hover .anchor .octicon-link,.markdown-body .h4:hover .anchor .octicon-link,.markdown-body .h5:hover .anchor .octicon-link,.markdown-body .h6:hover .anchor .octicon-link {
	visibility:visible
}
.markdown-body .h1 .tt,.markdown-body .h1 .code,.markdown-body .h2 .tt,.markdown-body .h2 .code,.markdown-body .h3 .tt,.markdown-body .h3 .code,.markdown-body .h4 .tt,.markdown-body .h4 .code,.markdown-body .h5 .tt,.markdown-body .h5 .code,.markdown-body .h6 .tt,.markdown-body .h6 .code {
	font-size:inherit
}
.markdown-body .h1 {
	margin-top:12rpx;
	padding-bottom:0.3em;
	font-size:2em;
	border-bottom:1rpx solid rgba(0, 0, 0, .05)
}
.markdown-body .h2 {
	margin-top:12rpx;
	padding-bottom:0.3em;
	font-size:1.5em;
	border-bottom:1rpx solid rgba(0, 0, 0, .05);
    display: inline-block;
}
.markdown-body .h3 {
	font-size:1.25em
}
.markdown-body .h4 {
	font-size:1em
}
.markdown-body .h5 {
	font-size:0.875em
}
.markdown-body .h6 {
	font-size:0.85em;
	color:#666
}
.markdown-body .ul_li_p::before{
	content:'• ';
}
.markdown-body .ul_li_ul_li_p::before,
.markdown-body .ol_li_ul_li_p::before{
	content:'◦ ';
}
.markdown-body .ul_li_ul_li_p,
.markdown-body .ul_li_ol_li_p,
.markdown-body .ol_li_ul_li_p,
.markdown-body .ol_li_ol_li_p{
	padding-left: 1em;
}
.markdown-body .ul_li_p:last{
	margin-bottom:1em;
}
.markdown-body .dl {
	padding:0
}
.markdown-body .dl .dt {
	padding:0;
	margin-top:32rpx;
	font-size:1em;
	font-style:italic;
	font-weight:bold
}
.markdown-body .dl dd {
	padding:0 32rpx;
	margin-bottom:32rpx
}
.markdown-body .table {
	display:block;
	width:100%;
	overflow:auto
}
.markdown-body .table .th {
	font-weight:bold
}
.markdown-body .table .th,.markdown-body .table .td {
	padding:12rpx 26rpx;
	border:1rpx solid #ddd
}
.markdown-body .table .tr {
	background-color:#fff;
	border-top:1rpx solid #ccc
}
.markdown-body .table .tr:nth-child(2n) {
	background-color:#f8f8f8
}
.markdown-body .img {
	width:100%;
	height: auto;
	display: block;
	margin: 1em 0;
	box-sizing:content-box;
	background-color:#fff;
	border-radius: 8rpx;
	-user-select: none;
	-webkit-user-select: none;
}
.markdown-body .img[align=right] {
	padding-left:40rpx
}
.markdown-body .img[align=left] {
	padding-right:40rpx
}
.markdown-body .emoji {
	max-width:none;
	vertical-align:text-top;
	background-color:transparent
}
.markdown-body .span.frame {
	display:block;
	overflow:hidden
}
.markdown-body .span.frame>.span {
	display:block;
	float:left;
	width:auto;
	padding:14rpx;
	margin:26rpx 0 0;
	overflow:hidden;
	border:1rpx solid #ddd
}
.markdown-body .span.frame .span .img {
	display:block;
	float:left
}
.markdown-body .span.frame .span .span {
	display:block;
	padding:10rpx 0 0;
	clear:both;
	color:#333
}
.markdown-body .span.align-center {
	display:block;
	overflow:hidden;
	clear:both
}
.markdown-body .span.align-center>.span {
	display:block;
	margin:26rpx auto 0;
	overflow:hidden;
	text-align:center
}
.markdown-body .span.align-center .span .img {
	margin:0 auto;
	text-align:center
}
.markdown-body .span.align-right {
	display:block;
	overflow:hidden;
	clear:both
}
.markdown-body .span.align-right>.span {
	display:block;
	margin:26rpx 0 0;
	overflow:hidden;
	text-align:right
}
.markdown-body .span.align-right .span .img {
	margin:0;
	text-align:right
}
.markdown-body .span.float-left {
	display:block;
	float:left;
	margin-right:26rpx;
	overflow:hidden
}
.markdown-body .span.float-left .span {
	margin:26rpx 0 0
}
.markdown-body .span.float-right {
	display:block;
	float:right;
	margin-left:26rpx;
	overflow:hidden
}
.markdown-body .span.float-right>.span {
	display:block;
	margin:26rpx auto 0;
	overflow:hidden;
	text-align:right
}
.markdown-body .code,.markdown-body .tt {
	font: 95% "Source Code Pro", Consolas, "Liberation Mono", Menlo, Courier, monospace, "PingFang SC", "Microsoft YaHei UI";
	padding:0;
	padding-top:0.1em;
	padding-bottom:0.2em;
	margin:0 5rpx;
	background-color: rgba(0, 0, 0, .05);
	border-radius:6rpx
}
.markdown-body .code.block {
	padding:0 1em;
}
.markdown-body .code::before,.markdown-body .code::after,.markdown-body .tt::before,.markdown-body .tt::after {
	letter-spacing:-0.2em;
	content:"\00a0"
}
.markdown-body .code .br,.markdown-body .tt .br {
	display:none
}
.markdown-body .deleted {
	text-decoration: line-through;
    border-radius: 6rpx;
}
.markdown-body .deleted .code {
	text-decoration:inherit;
    margin: 0;
    background:none
}
.markdown-body .pre {
	word-wrap:normal
}
.markdown-body .pre>.code {
	padding:0;
	margin:0;
	font-size:100%;
	word-break:normal;
	white-space:pre;
	background:transparent;
	border:0;
	
}
.markdown-body .highlight {
	margin-bottom:32rpx
}
.markdown-body .highlight .pre {
	margin-bottom:0;
	word-break:normal
}
.markdown-body .highlight .pre,.markdown-body .pre {
	padding:32rpx;
	overflow:auto;
	font-size:95%;
	line-height:1.45;
	background-color: rgba(0, 0, 0, .05);
	border-radius:6rpx
}
.markdown-body .pre {
	background-color: #f7f7f7;
}
.markdown-body .pre .code,.markdown-body .pre .tt {
	display:inline;
	max-width:auto;
	padding:0;
	margin:0;
	overflow:visible;
	line-height:inherit;
	word-wrap:normal;
	background-color:transparent;
	border:0
}
.markdown-body .pre .code::before,.markdown-body .pre .code::after,.markdown-body .pre .tt::before,.markdown-body .pre .tt::after {
	content:normal
}
.markdown-body .csv-data .td,.markdown-body .csv-data .th {
	padding:10rpx;
	overflow:hidden;
	font-size:24rpx;
	line-height:1;
	text-align:left;
	white-space:nowrap
}
.markdown-body .csv-data .blob-num {
	padding:20rpx 16rpx 18rpx;
	text-align:right;
	background:#fff;
	border:0
}
.markdown-body .csv-data .tr {
	border-top:0
}
.markdown-body .csv-data .th {
	font-weight:bold;
	background:#f8f8f8;
	border-top:0
}
